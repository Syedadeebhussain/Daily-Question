class Solution {
    public int maxDepth(String s) {
      int var=0;
      int max=0;
      for(int i=0;i<s.length();i++){
        if(s.charAt(i)=='('){
            var++;
            max=Math.max(max,var);
        }
        else if(s.charAt(i)==')'){
            var--;
        }
      }  
      return max;
    }
}
