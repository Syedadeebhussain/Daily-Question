/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode(int x) { val = x; }
 * }
 */

class Solution {
    public final TreeNode getTargetCopy(final TreeNode original, final TreeNode cloned, final TreeNode target) {
        return Solve(original,cloned,target);
    }
    public TreeNode Solve(final TreeNode original,final TreeNode cloned,final TreeNode target){
        if(original==null) return null;
        if(original==target){
            return cloned;
        }
       TreeNode l= Solve(original.left,cloned.left,target);
       if(l!=null) return l;
       return Solve(original.right,cloned.right,target);

    }
}
