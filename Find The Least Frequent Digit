class Solution {
    public int getLeastFrequentDigit(int n) {
    int [] freq=new int [10];
    int num=n;
    while(num!=0){
        int re=num%10;
        freq[re]++;
        num/=10;
    }
    int min=Integer.MAX_VALUE;
    for(int i=0;i<10;i++){
        if(freq[i] > 0 && freq[i]<min){
            min=freq[i];
        }
    }
    for(int i=0;i<10;i++){
        if(freq[i]==min){
            return i;
        }
    }
        return -1;
        }
}
