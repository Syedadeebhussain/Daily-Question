class Solution {
    public String stringHash(String s, int k) {
    StringBuilder st=new StringBuilder();
   for(int i=0;i<s.length();i+=k){
   String sub=s.substring(i,i+k);
   int sum=0;
   for(char ch:sub.toCharArray()){
   sum+=(ch-'a');
   }
    int ans=sum%26;
   char c=(char)('a'+ans);
   st.append(c);

   }
   return st.toString();
    }
}
