class Solution {
    public int sumDivisibleByK(int[] nums, int k) {
      int [] freq=new int [101];
        for(int i=0;i<nums.length;i++){
            freq[nums[i]]++;
        }
        int s=0;
        for(int i=0;i<freq.length;i++){
            if(freq[i]%k==0){
                s+=freq[i]*i;
            }
        }
        return s;
    }
}
